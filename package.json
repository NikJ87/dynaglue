{
  "name": "dynaglue",
  "version": "1.0.0",
  "description": "dynaglue",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile:build": "node esbuild.config.js",
    "compile:declarations": "tsc --build src",
    "build": "run-p compile:build compile:declarations",
    "prepublishOnly": "npm run build",
    "doc": "typedoc --tsconfig src/tsconfig.json src/index.ts",
    "lint": "eslint '**/*.ts'",
    "test": "jest",
    "prettier:check": "prettier -c **/*.ts",
    "prettier:write": "prettier --write **/*.ts",
    "test:watch": "jest --watch"
  },
  "author": "Chris Armstrong",
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.50.0",
    "@swc/core": "^1.3.67",
    "@swc/jest": "^0.2.26",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "dynalite": "^3.2.1",
    "esbuild": "^0.16.13",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.44.0",
    "jest": "^29.6.0",
    "jest-dynalite": "^3.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.22.11",
    "typescript": "~4.2.2"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.50.0"
  },
  "dependencies": {
    "@aws-sdk/util-dynamodb": "^3.50.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.20.21",
    "@types/validator": "^13.6.3",
    "@types/verror": "^1.10.5",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "validator": "^13.6.0",
    "verror": "^1.10.0"
  },
  "repository": {
    "url": "https://github.com/chris-armstrong/dynaglue.git"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
